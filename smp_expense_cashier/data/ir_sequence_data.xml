<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <record id="seq_hr_expense_sheet" model="ir.sequence">
            <field name="name">Feuille de dépenses</field>
            <field name="code">hr.expense.sheet</field>
            <field name="prefix">DEP/%(year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">6</field>
        </record>

        <record id="seq_hr_statement" model="ir.sequence">
            <field name="name">Relevé bancaire</field>
            <field name="code">account.bank.statement</field>
            <field name="prefix">CS/%(year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">6</field>
        </record>

    </data>
</odoo>

